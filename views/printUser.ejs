<!DOCTYPE html>
<html>
<head>
  <link rel="stylesheet" type="text/css" href="main.css">
  <script src="http://code.jquery.com/jquery-1.11.1.min.js"></script>
  <script src = "login.js" type="text/javascript"></script>
</head>
<body>
    <div>
    <table class="w3-table-all w3-card-4 w3-hoverable">
        <tbody class="w3-padding-large">
        <tr>
            <th>User Name</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Gender</th>
            <th>Mail</th>
            <th>Phone</th>
        </tr>
        <% for(var i = 0;i < userlist.length; i++) { %>
        <tr>
            <td><%= userlist[i].name %></td>
            <td><%= userlist[i].first_name %></td>
            <td><%= userlist[i].last_name %></td>
            <td><%= userlist[i].gender %></td>
            <td><%= userlist[i].mail %></td>
            <td><%= userlist[i].phone %></td>
        </tr>
        <% } %>
    </tbody>
    </table>
    <a href="./printUser.ejs" class="w3-btn w3-blue" id="downloadBtn" download>Download User List in PDF</a></p>
    </div>
</body>
</html>